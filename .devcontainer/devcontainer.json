{
    "name": "dependencies",
    "dockerFile": "Dockerfile",
    "postCreateCommand": "",
    "extensions": [
        // Other helpers
        "shardulm94.trailing-spaces",
        "stkb.rewrap" // rewrap comments after n characters on one line
    ],
    "settings": {
        // General settings
        "files.eol": "\n",
        "rewrap.autoWrap.enabled": true,
        "editor.formatOnSave": false
    }
}